{
  "extends": [
    "prettier",
    "airbnb"
  ],
  "rules": {
    "arrow-parens": ["error", "as-needed"],
    "linebreak-style": 0,
    "no-bitwise": 0,
    "function-paren-newline": 0,
    "prefer-destructuring": 0,
    "no-console": 0,
    "no-param-reassign": 0,
    "global-require" : 0,
    "dot-notation": 0,
    "no-plusplus" : 0,
    "no-restricted-globals": 0,
    "no-else-return": 0,
    "no-await-in-loop": 0
  }
}
